{
  "name": "Vute Client Ember",
  "description": "Client application for Vute.",
  "repository": "https://github.com/lapico/vute-client-ember",
  "logo": "https://raw.githubusercontent.com/lapico/vute-server-rails/master/public/vute_logo_grey.jpg",
  "keywords": ["vute", "ember"],
  "buildpacks": [{"url": "https://github.com/tonycoco/heroku-buildpack-ember-cli"}],
  "env": {
    "API_URL": {
      "description": "The url of Vute server application.",
      "value": "https://vute-server.herokuapp.com"
    },
    "API_VERSION": {
      "description": "The version of Vute server application.",
      "value": "api/v1"
    }
  }
}
