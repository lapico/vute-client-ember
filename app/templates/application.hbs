<h1>Vute Ember Client</h1>

<h2>application template</h2>

{{#if session.isAuthenticated}}
  <p> Current user : {{session.currentUser.name}} </p>
  <p><a href="#" {{action 'invalidateSession'}} >Logout</a></p>
{{else}}
  <p> {{link-to "Login" 'login'}} </p>
  <p> {{link-to "Sign up" 'users.new'}} </p>
{{/if}}

{{outlet}}
